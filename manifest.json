{
  "name": "Shrink... the link.",
  "description": "Nobody reads descriptions bruh.",
  "version": "1.0",
  "icons": {
    "128": "res/128icon.png",
    "48": "res/48icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": ["jquery-3.2.1.min.js","js/bg.js"],
      "run_at": "document_start"
    }],
  "permissions": [
    "activeTab",
    "https://shrinknation/summary/",
    "tabs"
  ],

  "browser_action": {
      "default_title": "For Later",
      "default_icon": "res/icon.png"
  },
  "manifest_version": 2
}
